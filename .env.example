# PROVES Library Environment Variables

# Database
NEON_DATABASE_URL=postgresql://user:password@host/dbname

# LangSmith (for tracing and observability)
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=PROVES_Library
# Optional: If your API key is linked to multiple workspaces
# LANGSMITH_WORKSPACE_ID=your_workspace_id

# OpenAI (for LLM calls and embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# GitHub (for documentation fetching)
GITHUB_TOKEN=your_github_token_here
